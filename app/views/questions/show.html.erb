<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @question.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @question.body %>
</p>

<h2>List of Contributors</h2>

<tbody>
  <% @questions.each do |question| %>
    <tr>
      <td>@question.user.email</td>
    </tr>
  <% end %>
</tbody>


<%= link_to 'Edit', edit_question_path(@question) %> |
<%= link_to 'Back', questions_path %>
